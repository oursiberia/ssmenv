{
  "name": "ssmenv",
  "description": "Manage environment variables with AWS SSM.",
  "version": "0.1.2",
  "author": "Bryan Swift",
  "bin": {
    "ssmenv": "./bin/run"
  },
  "bugs": "https://github.com/oursiberia/ssmenv/issues",
  "dependencies": {
    "@oclif/command": "^1.4.7",
    "@oclif/config": "^1.3.62",
    "@oclif/parser": "^3.2.11",
    "@oclif/plugin-help": "^1.2.2",
    "aws-sdk": "^2.222.1",
    "chalk": "^2.3.2",
    "inquirer": "^5.2.0",
    "lru-cache": "^4.1.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.4.4",
    "@oclif/tslint": "^1.1.0",
    "@types/chai": "^4.1.2",
    "@types/inquirer": "^0.0.41",
    "@types/lru-cache": "^4.1.0",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "globby": "^8.0.1",
    "pkg": "^4.3.1",
    "prettier": "^1.11.1",
    "ts-node": "5",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/oursiberia/ssmenv",
  "keywords": [
    "oclif",
    "aws",
    "ssm"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ssmenv",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "oursiberia/ssmenv",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "dist": "yarn run build && yarn pkg --targets=node8-macos,node8-linux,node8-windows --out-path=dist .",
    "lint": "tsc -p . --noEmit && tslint -p . -t stylish",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && oclif-dev manifest && oclif-dev readme",
    "preversion": "yarn run clean",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "pkg": {
    "scripts": "lib/**/*.js"
  }
}
